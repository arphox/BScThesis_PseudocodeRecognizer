<Window x:Class="VisualParseTree.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Loaded="Window_Loaded" 
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TreeView x:Name="TreeView" Margin="10"/>
        <Button Content="Expand all" HorizontalAlignment="Right" Margin="0,10,30,0" VerticalAlignment="Top" Width="84" Height="24" Click="ButtonExpandAll_Click"/>
        <Button Content="Collapse all" HorizontalAlignment="Right" Margin="0,39,30,0" VerticalAlignment="Top" Width="84" Height="24" Click="ButtonCollapseAll_Click"/>
        <Button Content="Print leaves" HorizontalAlignment="Right" Margin="0,68,30,0" VerticalAlignment="Top" Width="84" Height="24" Click="ButtonPrintLeaves_Click"/>
    </Grid>
</Window>
